<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:tooltipText="主管界面"
    tools:context="com.example.dell.demo2.supervisor.SupervisorActivity">
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <Button
        android:id="@+id/btn_getLimit"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:text="工作人员任务分配"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="42dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_getMeetingDetail"
        android:layout_width="301dp"
        android:layout_height="wrap_content"
        android:text="查看会议准备情况"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="101dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_putMsg"
        android:layout_width="306dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:text="发布消息"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="159dp"
        tools:ignore="MissingConstraints"
        android:layout_marginRight="36dp" />

    <Button
        android:id="@+id/btn_pendingApplication"
        android:layout_width="307dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="待审批事项"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="222dp"
        tools:ignore="MissingConstraints"
        android:layout_marginLeft="36dp" />

    <Button
        android:id="@+id/btn_uploadFiles"
        android:layout_width="301dp"
        android:layout_height="wrap_content"
        android:text="上传文件"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="278dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_callSecretary"
        android:layout_width="294dp"
        android:layout_height="wrap_content"
        android:text="联系秘书"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="333dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_goWorkerTalk"
        android:layout_width="291dp"
        android:layout_height="wrap_content"
        android:text="工作人员讨论区"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="394dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_goConsultTalk"
        android:layout_width="291dp"
        android:layout_height="wrap_content"
        android:text="坐席解答区"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="447dp"
        tools:ignore="MissingConstraints" />
</LinearLayout>

</android.support.constraint.ConstraintLayout>
